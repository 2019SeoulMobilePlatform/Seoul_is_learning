<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clubactivity">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_seoul"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:screenOrientation="portrait"
        android:roundIcon="@mipmap/ic_launcher_seoul_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".MyPage.EnterEditInfoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat"></activity>
        <activity
            android:name=".Club.FullScreenImageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Instructor.AddClassActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".EditClassActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Instructor.InstructorMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Club.ClubEnterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Class.ReviewAddActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".MyPage.EditMyInfoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Login.FindPasswordAcitvity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Login.SignUpActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Login.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Club.ChatRoomActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Club.AddClubActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Home.DetailActivity" />
        <activity
            android:name=".Class.ClassDetailActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:screenOrientation="portrait"
            android:name=".Class.ClassReservation" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Home.WebViewActivity" />
    </application>

</manifest>