<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clubactivity">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_seoul"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_seoul_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">

        <activity android:name=".Login.FindPasswordAcitvity"
            android:theme="@style/NoTitleAppcompat"></activity>
        <activity
            android:name=".Login.SignUpActivity"
            android:theme="@style/NoTitleAppcompat" />
        <activity
            android:name=".Login.LoginActivity"
            android:theme="@style/NoTitleAppcompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ChatRoomActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".AddClubActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".MainActivity"

            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Home.DetailActivity" />


        <activity android:name=".Class.ClassDetailActivity"
            android:theme="@style/NoTitleAppcompat">
        </activity>

        <activity android:name=".Class.ClassReservation">

        </activity>


    </application>

</manifest>